{
	"name": "release cors",
	"version": "1.0.4",
	"manifest_version": 2,
	"description": "解除HTTP访问控制（CORS）的各种限制,requestHeader和responeHeader各种可配置化",
	"background": {
		"scripts": ["background.js"]
	},
	"browser_action": {
		"default_icon": "assets/images/off.png",
		"default_title": "Allow-Control-Allow-Origin",
		"default_popup": "popup.html"
	},
	"default_locale": "en",
	"permissions": [
		"storage",
		"webRequest",
		"webRequestBlocking",
		"<all_urls>"
	],
	"web_accessible_resources": [
		"assets/images/on.png","assets/images/off.png"
	]
}